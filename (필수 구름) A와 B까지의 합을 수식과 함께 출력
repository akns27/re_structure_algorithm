//쌤의 풀이
#include<stdio.h>
int main(){
	int a,b,sum=0,i;
	scanf("%d %d", &a, &b);
	for(i = a; i<b; i++){
		sum += i;
		printf("%d + ", i);
	}
	printf("%d = %d", i, sum+i);
	return 0;
}
